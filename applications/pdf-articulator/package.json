    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.3.3",
    "typescript": "5.9.2",
    "typescript-eslint": "^8",
    "vite": "^4.4.5"
